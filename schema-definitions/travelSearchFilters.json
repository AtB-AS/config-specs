{
  "$ref": "#/definitions/TravelSearchFilters",
  "definitions": {
    "LanguageAndTextTypeArray": {
      "type": "array",
      "items": {
        "anyOf": [
          {
            "type": "object",
            "properties": {
              "lang": {
                "type": "string"
              },
              "value": {
                "type": "string"
              }
            },
            "required": ["lang", "value"],
            "additionalProperties": false
          },
          {
            "type": "object",
            "properties": {
              "language": {
                "type": ["string", "null"]
              },
              "value": {
                "type": ["string", "null"]
              }
            },
            "additionalProperties": false
          }
        ]
      }
    },
    "TravelSearchTransportModes": {
      "type": "object",
      "properties": {
        "transportMode": {
          "$ref": "#/definitions/TransportModeType"
        },
        "transportSubModes": {
          "anyOf": [
            {
              "type": "array",
              "items": {
                "$ref": "#/definitions/TransportSubmodeType"
              }
            },
            {
              "type": "null"
            }
          ]
        }
      },
      "required": ["transportMode"],
      "additionalProperties": false
    },
    "TravelSearchTransportModeIcon": {
      "type": "object",
      "properties": {
        "transportMode": {
          "$ref": "#/definitions/TransportModeType"
        },
        "transportSubMode": {
          "anyOf": [
            {
              "$ref": "#/definitions/TransportSubmodeType"
            },
            {
              "type": "null"
            }
          ]
        }
      },
      "required": ["transportMode"],
      "additionalProperties": false
    },
    "TransportModeType": {
      "type": "string",
      "enum": [
        "air",
        "bicycle",
        "bus",
        "cableway",
        "coach",
        "foot",
        "funicular",
        "lift",
        "metro",
        "monorail",
        "rail",
        "scooter",
        "taxi",
        "tram",
        "trolleybus",
        "unknown",
        "water"
      ]
