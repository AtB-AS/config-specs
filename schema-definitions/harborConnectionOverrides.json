{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "object",
  "properties": {
    "overrides": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "from": {
            "type": "string"
          },
          "to": {
            "type": "string"
          },
          "isFree": {
            "type": "boolean"
          }
        },
        "required": ["from", "to"],
        "additionalProperties": false
      }
    }
  },
  "required": ["overrides"],
  "additionalProperties": false
}
